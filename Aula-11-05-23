ex 2:

#include <stdio.h>

int RaioCircuferencia(int numero);

int main(){
	int raio, resutado0;
	printf("Informe o diametro da circuferencia: ");
	scanf("%d", &raio);
	
	resutado0 = RaioCircuferencia(raio);
	printf("O raio da circuferencia e: %d cm", resutado0);
	
	return 0;
}

int RaioCircuferencia(int numero){
	int resultado;
	
	resultado = numero / 2;
	return resultado;
}
