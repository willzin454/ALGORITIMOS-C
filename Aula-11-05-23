ex 3:

#include <stdio.h>

int CadeiaCaracter(char x[], char y);

int main(){
	int resultado0;
	char fist, letras[20];
	printf("Informe um caracter a comparar: ");
	scanf("%s", &fist);
	
	for(int i = 0; i < 20; i++){	
		printf("Informe o caracter N-%d :", i + 1);
		scanf("%s", &letras[i]);
		resultado0 = CadeiaCaracter(letras, fist);
	}
	printf("O total de caracteres iguais foi de: %d", resultado0);
	
	return 0;
}

int CadeiaCaracter(char x[], char y){
	int cont = 0;
	for(int i = 0; i < 20; i++){
		if(x[i] == y){
			cont+=1;
		}
	}
	return cont;			
}
