#include <stdio.h>

int main()
{
	int x[100];
	int i;
	for (i =0; i<100;++i){
		x[i] = i;
	}
	for (i = 0; i<100;++i){
		printf("%d\n",x[i]);
	}
	printf("\n");
	return 0;
}
