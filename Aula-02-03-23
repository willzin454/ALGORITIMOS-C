#include <stdio.h>

int main ()
{
	char nome[25];
	int i;
	printf ("Digite um nome: ");
	gets(nome);
	printf ("O nome que voce digitou e: ");
	puts(nome);
// ou printf ("O nome que voce digitou e: %s\n\n", nome);
	return 0;
}
------------------------------------------------
#include <stdio.h>

int main()
{
	int mat[3][3];
	for (int x = 0; x<3; x++){
		for (int y = 0; y<3;y++){
			printf("Informar um valor: ");
			scanf("%d", &mat[x][y]);
		}
	}
	for (int x = 0; x<3; x++){
		for (int y = 0; y<3;y++){
			if(x == y){
				printf("Elemento da diagonal principal: %d\n", mat[x][y]);
			}			
		}
	}
	return 0;
}
------------------------------------------------
#include <stdio.h>
#include <string.h>

int main()
{
	char nome1[20], nome2[20];
	printf("Infomre o primeiro nome: ");
	gets(nome1);	//gets sรณ funciona com str
	printf("Infomre o segundo nome: ");
	gets(nome2);
	if(strcmp(nome1, nome2) == 0){
		printf("Sao iguais");
	}
	else{
		printf("Sao diferentes");
	}
	return 0;
}
